{
  "name": "brain-script-lang",
  "displayName": "Brain Script Lang",
  "description": "Syntax highlighting for 3D Rad 5.2 Brain Scripting",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "brain",
      "aliases": ["3D Rad Brain Script", "brain"],
      "extensions": [".bra"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "brain",
      "scopeName": "source.brain",
      "path": "./syntaxes/brain.tmLanguage.json"
    }]
  }
}
